{
  "name": "fastro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "npx simple-git-hooks && npm run generate-schema",
    "sync-datocms": "node scripts/sync-datocms.mjs",
    "dev": "run-p dev:* --print-label",
    "dev:dato": "npm run sync-datocms",
    "dev:astro": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "generate-schema": "npx datocms schema:generate schema.ts",
    "lint": "prettier --check .",
    "format": "npx prettier . --write"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.0",
    "@datocms/astro": "^0.6.2",
    "@datocms/cda-client": "^0.2.6",
    "@datocms/cli": "^3.1.4",
    "@datocms/cma-client": "^4.0.1",
    "@mux/mux-player": "^2.9.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^5.8.2",
    "datocms-structured-text-generic-html-renderer": "^5.0.0",
    "datocms-structured-text-to-plain-text": "^5.0.0",
    "gql.tada": "^1.8.10",
    "jsdom": "^24.1.1",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "react-datocms": "^7.0.1",
    "react-dom": "^18.3.1",
    "serialize-error": "^11.0.3",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.6",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format"
  }
}
